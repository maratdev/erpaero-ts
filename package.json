{
  "name": "erpaero-ts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "tsx watch --include ./src --exclude ./src/**/*.spec.ts --clear-screen=false ./src/main.ts",
    "dev:inspect": "tsx --inspect-brk ./src/main.ts",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "build": "tsc && tsc-alias",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "~1.4.4",
    "debug": "4.3.5",
    "dotenv": "^16.4.5",
    "express": "5.0.0",
    "inversify": "^6.1.4",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "reflect-metadata": "^0.2.2",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}
